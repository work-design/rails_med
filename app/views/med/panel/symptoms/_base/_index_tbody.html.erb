<td><%= link_to model.name, { action: 'show', id: model.id } %></td>
<td>
  <p><%= model.type_i18n %></p>
  <p><%= model.bodily_i18n %></p>
</td>
<td>
  <div class="tags">
  <% model.bodies.each do |body| %>
    <span class="tag"><%= body.name %></span>
  <% end %>
  </div>
  <%= button_to({ action: 'bodies', id: model.id }, class: 'button is-small is-rounded is-light') do %>
    <i class="fa-solid fa-child"></i>
    <span class="ml-1"><%= t('.bodies.title') %></span>
  <% end unless model.whole? %>
</td>
<td>
  <% if model.diagnose %>
    <span><%= model.diagnose.name %></span>
    <%= link_to panel_model_diagnoses_path(model), aria: { label: t('med.panel.model_diagnoses.index.title') } do %>
      <i class="fa-light fa-microscope"></i>
    <% end %>
  <% end %>
</td>
<td>
  <div class="tags">
    <% model.causes.each do |cause| %>
      <span class="tag"><%= cause.name %></span>
    <% end %>
  </div>
  <%= link_to({ action: 'causes', id: model.id }, class: 'button is-small is-rounded is-light') do %>
    <i class="fa-solid fa-viruses"></i>
    <span class="ml-1"><%= t('.causes.title') %></span>
  <% end %>
</td>
<td>
  <%= form_with theme: nil, model: model, url: { action: 'update', id: model.id }, data: { controller: 'input' } do |f| %>
    <%= f.check_box :mutable, data: { input_target: 'checkbox' }, id: nil, class: 'switch is-rounded' %>
    <label data-action="click->input#check"></label>
  <% end %>
</td>
<td>
  <%= form_with theme: nil, model: model, url: { action: 'update', id: model.id }, data: { controller: 'input' } do |f| %>
    <%= f.check_box :primary, data: { input_target: 'checkbox' }, id: nil, class: 'switch is-rounded' %>
    <label data-action="click->input#check"></label>
  <% end %>
  <%= form_with theme: nil, model: model, url: { action: 'update', id: model.id }, data: { controller: 'input' } do |f| %>
    <%= f.check_box :physical, data: { input_target: 'checkbox' }, id: nil, class: 'switch is-rounded' %>
    <label data-action="click->input#check"></label>
  <% end %>
</td>
<td><%= model.sequence %></td>

