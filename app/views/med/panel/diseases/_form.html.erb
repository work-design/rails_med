<%= form_with model: [:panel, @disease] do |f| %>
  <%= render 'shared/error_messages', target: @disease %>
  <%= f.text_field :name %>
  <%= f.collection_select :symptom_ids, Symptom.all, :id, :name, { wrap: { select: 'field-body control' } }, { multiple: true, data: { controller: 'choice' } } %>
  <%= f.submit %>
<% end %>
